<%- include('layouts/user-header.ejs'); %>
        <div class="workout-header strength-background dark-gray-font">
            <h2><%- workoutName %></h2>
            <p>Phase: <%- userPhase %> <img class="workout-header-icon invert" src="/img/calendar-icon.svg" />Day <%- userCurrentDay %></p>
        </div>
        <div id="exercise-container" data-content="3">
            <div class="exercise-header">
                <p class="exercise-number">1</p>
                <div>
                    <p class="exercise-muscle-group"><%- exercises[0].muscleGroup %></p>
                    <h3><%- exercises[0].exercise %></h3>
                </div>
            </div>
            <div class="counter-container">
                <div class="button-container">
                    <div class="increase increase-decrease-button">+</div>
                    <div class="increase-decrease-button">&#8722;</div>
                </div>
                <div class="weight-reps">
                    <input type="number" placeholder="0" class="count" />
                    <p>weight</p>
                </div>
            </div>
            <!-- <div class="qty-quick-select">
                <ul>
                    <li>5</li>
                    <li>10</li>
                    <li>15</li>
                    <li>20</li>
                    <li>25</li>
                </ul>
            </div> -->
            <div class="counter-container">
                <div class="button-container">
                    <div class="increase increase-decrease-button">+</div>
                    <div class="increase-decrease-button">&#8722;</div>
                </div>
                <div class="weight-reps">
                    <input type="number" placeholder="0" class="count" />
                    <p>Reps</p>
                </div>
            </div>
            <!-- <div class="qty-quick-select">
                <ul>
                    <li>5</li>
                    <li>10</li>
                    <li>15</li>
                    <li>20</li>
                    <li>25</li>
                </ul>
            </div> -->
            <a class="strength-background" href="#">Next</a>
        </div>
        <div class="phase-workouts">
            <h3 class="phase-header">Full Workout <img src="/img/filter-icon.svg" /></h3>
            <ul>
                <% for (let i = 0; i < exercises.length; i++) { %>
                    <li class="exercise-lower-list"
                        <% if (exercises[i].weight) { %> data-weight="0" <% } %>
                        <% if (exercises[i].reps) { %> data-reps="0" <% } %>
                        <% if (exercises[i].time) { %> data-time="0" <% } %>
                        data-complete="0"
                        >
                        <p class="exercise-number"><%= (i + 1) %></p>
                        <div>
                            <p class="exercise-muscle-group"><%= exercises[i].muscleGroup %></p>
                            <h3><%= exercises[i].exercise %></h3>
                        </div>
                        <img class="" src="/img/incomplete-check-icon.svg" />
                    </li>
                <% } %>
            </ul>
    </div>
    </div>
<script src="/js/repsWeightCounter.js"></script>
<%- include ('layouts/general-footer.ejs'); %>